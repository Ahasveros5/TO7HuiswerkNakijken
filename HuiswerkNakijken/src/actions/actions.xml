<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

		<package name="brules" namespace="/brules" extends="struts-default">
		<action name="Generate" class="actions.brules.Generate">
			<result name="success">/jsp/brules/generator.jsp</result>
			<result name="input">/jsp/brules/generator.jsp</result>
		</action> 
		<action name="PushToTarget" class="actions.brules.Push">
			<result name="success">/jsp/brules/pushed.jsp</result>
		</action>
		</package>
	</struts>
